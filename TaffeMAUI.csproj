	  </MauiXaml>
	  <MauiXaml Update="Pages\ats\at8.xaml">
	    <Generator>MSBuild:Compile</Generator>
	  </MauiXaml>
	  <MauiXaml Update="Pages\ats\at9.xaml">
	    <Generator>MSBuild:Compile</Generator>
